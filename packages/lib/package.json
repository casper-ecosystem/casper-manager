{
  "name": "@casperholders/casper-manager-helper",
  "version": "1.0.6",
  "description": "Simple helper to use the casper manager",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "yarn run build:lib && yarn run build:types",
    "build:lib": "tsc --build",
    "build:types": "tsc --emitDeclarationOnly",
    "docs": "typedoc --plugin typedoc-plugin-missing-exports",
    "lint": "yarn run lint:style && yarn run lint:types",
    "lint:style": "eslint --color 'src/**/*.{js,ts,tsx}'",
    "lint:style:fix": "yarn run lint:style --fix",
    "lint:types": "tsc --noEmit --pretty",
    "release": "release-it",
    "test": "exit 0"
  },
  "dependencies": {
    "@metamask/providers": "11.0.0",
    "buffer": "6.0.3",
    "casper-js-sdk": "2.13.2"
  },
  "devDependencies": {
    "esdoc-typescript-plugin": "1.0.1",
    "release-it": "15.11.0",
    "typedoc": "0.24.8",
    "typedoc-plugin-missing-exports": "2.0.0"
  }
}
