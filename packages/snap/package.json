{
  "name": "@casperholders/casper-manager",
  "version": "1.0.6",
  "description": "Sign deploys and messages for the Casper Blockchain with your Casper account(s).",
  "repository": {
    "type": "git",
    "url": "https://github.com/casperholders/casper-snap.git"
  },
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "build:website": "node ./scripts/build-website.js",
    "clean": "rimraf dist",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "release": "release-it",
    "serve": "mm-snap serve -p 9000",
    "start": "mm-snap watch -p 9000",
    "test": "jest --silent --runInBand"
  },
  "dependencies": {
    "@ethersproject/bignumber": "5.7.0",
    "@metamask/key-tree": "7.1.1",
    "@metamask/snaps-ui": "0.32.2",
    "casper-js-sdk": "2.13.2",
    "ethereum-cryptography": "2.0.0",
    "tweetnacl-ts": "1.0.3"
  },
  "devDependencies": {
    "@chainsafe/dappeteer": "5.2.0",
    "@lavamoat/allow-scripts": "2.3.1",
    "@metamask/auto-changelog": "3.1.0",
    "@metamask/eslint-config": "11.1.0",
    "@metamask/eslint-config-jest": "11.1.0",
    "@metamask/eslint-config-nodejs": "11.1.0",
    "@metamask/eslint-config-typescript": "11.1.0",
    "@metamask/snaps-cli": "0.32.2",
    "@metamask/snaps-types": "0.32.2",
    "@types/jest": "29.5.2",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jsdoc": "46.2.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "playwright": "1.35.1",
    "prettier": "2.8.8",
    "prettier-plugin-packagejson": "2.4.3",
    "release-it": "15.11.0",
    "rimraf": "5.0.1",
    "ts-results": "npm:@casperlabs/ts-results@3.3.5",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@3.6.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
